{
  "name": "captain-fact-overlay-injector",
  "version": "0.8.1",
  "description": "http://captainfact.io - Injects facts overlays ",
  "main": "dist/captain-fact-overlay-injector.js",
  "scripts": {
    "build": "webpack --env production",
    "build_dev": "webpack --env dev",
    "start": "webpack-dev-server --env dev",
    "startp": "webpack-dev-server --env production",
    "watch": "webpack -w",
    "watchp": "webpack -w -p",
    "test": "jest",
    "wtest": "jest --watch",
    "coverage": "jest --coverage '--collectCoverageFrom=src/**/!(record).{js,jsx}'"
  },
  "jest": {
    "setupFiles": [
      "./tests_setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/constants/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png)$": "<rootDir>/src/__mocks__/fileMock.js",
      "env-constants": "<rootDir>/constants/test.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainFact/captain-fact-overlay-injector"
  },
  "author": "Benjamin Piouffle <benjamin.piouffle@gmail.com>",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "compression-webpack-plugin": "^1.1.3",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.8",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.6",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.10",
    "react-addons-test-utils": "^15.0.2",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "regenerator-runtime": "^0.11.1",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "debounce": "^1.1.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jumpstate": "^2.2.2",
    "react": "^16.2.0",
    "react-dock": "^0.2.3",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "voca": "^1.3.0"
  }
}
