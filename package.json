{
  "name": "captain-fact-overlay-injector",
  "version": "2.0.0",
  "description": "http://captainfact.io - Injects facts overlays ",
  "main": "dist/captain-fact-overlay-injector.js",
  "scripts": {
    "build": "webpack --env production",
    "build_dev": "webpack --env dev",
    "start": "webpack-dev-server --env dev",
    "dev": "webpack-dev-server --env dev",
    "startp": "webpack-dev-server --env production",
    "watch": "webpack -w",
    "watchp": "webpack -w -p",
    "test": "jest",
    "wtest": "jest --watch",
    "coverage": "jest --coverage '--collectCoverageFrom=src/**/!(record).{js,jsx}'",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix && npm run prettier",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check"
  },
  "jest": {
    "setupFiles": [
      "./dev/tests_setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/constants/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "env-constants": "<rootDir>/constants/test.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainFact/captain-fact-overlay-injector"
  },
  "author": "Benjamin Piouffle <benjamin.piouffle@gmail.com>",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-loader": "^9.1.2",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "coveralls": "^3.0.3",
    "css-loader": "6.7.3",
    "css-modules-require-hook": "^4.2.3",
    "eslint": "^8.37.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-serializer-html-string": "^1.0.1",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "style-loader": "3.3.2",
    "svg-inline-loader": "^0.8.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "classnames": "^2.1.3",
    "debounce": "^1.2.0",
    "i18next": "^22.4.13",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^3.0.0",
    "jest-cli": "^29.5.0",
    "jumpstate": "^2.2.2",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "reselect": "^4.0.0",
    "voca": "^1.3.0"
  }
}
