{
  "name": "captain-fact-overlay-injector",
  "version": "0.8.13",
  "description": "http://captainfact.io - Injects facts overlays ",
  "main": "dist/captain-fact-overlay-injector.js",
  "scripts": {
    "build": "webpack --env production",
    "build_dev": "webpack --env dev",
    "start": "webpack-dev-server --env dev",
    "dev": "webpack-dev-server --env dev",
    "startp": "webpack-dev-server --env production",
    "watch": "webpack -w",
    "watchp": "webpack -w -p",
    "test": "jest",
    "wtest": "jest --watch",
    "coverage": "jest --coverage '--collectCoverageFrom=src/**/!(record).{js,jsx}'",
    "lint": "./node_modules/.bin/eslint src",
    "lint_fix": "./node_modules/.bin/eslint src --fix"
  },
  "jest": {
    "setupFiles": [
      "./dev/tests_setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/constants/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "env-constants": "<rootDir>/constants/test.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptainFact/captain-fact-overlay-injector"
  },
  "author": "Benjamin Piouffle <benjamin.piouffle@gmail.com>",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-react-optimize": "^1.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.3",
    "coveralls": "^3.0.3",
    "css-loader": "^1.0.0",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-preact": "^0.2.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-serializer-html-string": "^1.0.1",
    "postcss-loader": "^2.1.5",
    "preact-compat-enzyme": "^0.2.5",
    "preact-render-to-string": "^4.1.0",
    "preact-test-utils": "^0.1.3",
    "preact-transition-group": "^1.1.1",
    "react-addons-test-utils": "^15.0.2",
    "redux-mock-store": "^1.4.0",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "debounce": "^1.2.0",
    "i18next": "^13.1.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "^24.5.0",
    "jumpstate": "^2.2.2",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "react-dock": "^0.2.3",
    "react-i18next": "^8.0.7",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "reselect": "^4.0.0",
    "voca": "^1.3.0"
  },
  "greenkeeper": {
    "ignore": [
      "enzyme",
      "postcss-loader"
    ]
  }
}
